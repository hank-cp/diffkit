<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="plan" class="org.diffkit.diff.conf.DKPassthroughPlan">
		<property name="lhsSource" ref="lhs.source" />
		<property name="rhsSource" ref="rhs.source" />
		<property name="sink" ref="sink.db" />
		<property name="tableComparison" ref="table.comparison" />
	</bean>

	<bean id="table.comparison" class="org.diffkit.diff.custom.DKCustomTableComparison">
		<constructor-arg index="0" ref="lhs.table.model" />
		<constructor-arg index="1" ref="rhs.table.model" />
		<constructor-arg index="2" value="BOTH" />
		<constructor-arg index="3">
			<list>
				<ref bean="column.comparison.order_code" />
				<ref bean="column.comparison.seq" />
				<ref bean="column.comparison.code" />
				<ref bean="column.comparison.name" />
				<ref bean="column.comparison.spec" />
				<ref bean="column.comparison.count" />
				<ref bean="column.comparison.ware_house" />
				<ref bean="column.comparison.delivery_date" />
			</list>
		</constructor-arg>
		<constructor-arg index="4">
			<list>
				<value>0</value>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
				<value>5</value>
				<value>6</value>
				<value>7</value>
			</list>
		</constructor-arg>
		<constructor-arg index="5">
			<list>
				<list>
					<value>0</value>
					<value>1</value>
					<value>2</value>
					<value>3</value>
					<value>4</value>
					<value>5</value>
					<value>6</value>
					<value>7</value>
				</list>
				<list>
					<value>0</value>
					<value>1</value>
					<value>2</value>
					<value>3</value>
					<value>4</value>
					<value>5</value>
					<value>6</value>
					<value>7</value>
				</list>
			</list>
		</constructor-arg>
		<constructor-arg index="6" value="9223372036854775807" />
		<!--<constructor-arg index="6" value="10"/>-->
	</bean>

	<bean id="column.comparison.order_code" class="org.diffkit.diff.engine.DKColumnComparison">
		<constructor-arg index="0" ref="lhs.column.order_code" />
		<constructor-arg index="1" ref="rhs.column.order_code" />
		<constructor-arg index="2" ref="equalsDiffor" />
	</bean>

	<bean id="column.comparison.seq" class="org.diffkit.diff.engine.DKColumnComparison">
		<constructor-arg index="0" ref="lhs.column.seq" />
		<constructor-arg index="1" ref="rhs.column.seq" />
		<constructor-arg index="2" ref="numberDiffor" />
	</bean>

	<bean id="column.comparison.code" class="org.diffkit.diff.engine.DKColumnComparison">
		<constructor-arg index="0" ref="lhs.column.code" />
		<constructor-arg index="1" ref="rhs.column.code" />
		<constructor-arg index="2" ref="equalsDiffor" />
	</bean>

	<bean id="column.comparison.name" class="org.diffkit.diff.engine.DKColumnComparison">
		<constructor-arg index="0" ref="lhs.column.name" />
		<constructor-arg index="1" ref="rhs.column.name" />
		<constructor-arg index="2" ref="equalsDiffor" />
	</bean>

	<bean id="column.comparison.spec" class="org.diffkit.diff.engine.DKColumnComparison">
		<constructor-arg index="0" ref="lhs.column.spec" />
		<constructor-arg index="1" ref="rhs.column.spec" />
		<constructor-arg index="2" ref="equalsDiffor" />
	</bean>

	<bean id="column.comparison.count" class="org.diffkit.diff.engine.DKColumnComparison">
		<constructor-arg index="0" ref="lhs.column.count" />
		<constructor-arg index="1" ref="rhs.column.count" />
		<constructor-arg index="2" ref="equalsDiffor" />
	</bean>

	<bean id="column.comparison.ware_house" class="org.diffkit.diff.engine.DKColumnComparison">
		<constructor-arg index="0" ref="lhs.column.ware_house" />
		<constructor-arg index="1" ref="rhs.column.ware_house" />
		<constructor-arg index="2" ref="equalsDiffor" />
	</bean>

	<bean id="column.comparison.delivery_date" class="org.diffkit.diff.engine.DKColumnComparison">
		<constructor-arg index="0" ref="lhs.column.delivery_date" />
		<constructor-arg index="1" ref="rhs.column.delivery_date" />
		<constructor-arg index="2" ref="equalsDiffor" />
	</bean>

	<bean id="equalsDiffor" class="org.diffkit.diff.diffor.DKEqualsDiffor"
		factory-method="getInstance" />

	<bean id="numberDiffor" class="org.diffkit.diff.diffor.DKNumberDiffor"
		  factory-method="getInstance" />

	<bean id="sink.db" class="org.diffkit.diff.custom.DKCustomDBSink">
    	<constructor-arg index="0" value="./proofhead.end_product.diff" />
		<constructor-arg index="1" ref="sinkConnectionSource" />
		<constructor-arg index="2" ref="table.comparison" />
		<constructor-arg index="3" value="diff_end_product" />
		<constructor-arg index="4" value="ENGINE=MyISAM" />
		<constructor-arg index="5" ref="lhs.source" />
		<constructor-arg index="6" value="0" />
		<constructor-arg index="7" ref="lhs.source" />
		<constructor-arg index="8" ref="rhs.source" />
		<constructor-arg index="9" value="" />
		<constructor-arg index="10" value="" />
		<constructor-arg index="11" value="" />
		<constructor-arg index="12" ><null/></constructor-arg>
	</bean>

	<!-- ATS database config -->
	<bean id="lhs.source" class="org.diffkit.diff.sns.DKDBSource">
		<constructor-arg index="0" value="po_end_product_proofhead_view" />
		<constructor-arg index="1" value="WHERE ORDER_STATUS = 4" />
		<constructor-arg index="2" ref="atsConnectionSource" />
		<constructor-arg index="3" ref="lhs.table.model" />
		<constructor-arg index="4">
			<list>
				<value>ORDER_CODE</value>
				<value>CODE</value>
			</list>
		</constructor-arg>
		<constructor-arg index="5"><null /></constructor-arg>
	</bean>
	
	<bean id="lhs.table.model" class="org.diffkit.diff.engine.DKTableModel">
        <constructor-arg index="0" value="ng_proofhead_product_view" />
		<constructor-arg index="1">
			<list>
				<ref bean="lhs.column.order_code" />
				<ref bean="lhs.column.seq" />
				<ref bean="lhs.column.code" />
				<ref bean="lhs.column.name" />
				<ref bean="lhs.column.spec" />
				<ref bean="lhs.column.count" />
				<ref bean="lhs.column.ware_house" />
				<ref bean="lhs.column.delivery_date" />
			</list>
		</constructor-arg>
		<constructor-arg index="2">
			<list>
				<value>0</value>
			</list>
		</constructor-arg>
	</bean>

	<bean id="lhs.column.order_code" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="0" />
		<constructor-arg index="1" value="ORDER_CODE" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="lhs.column.seq" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="1" />
		<constructor-arg index="1" value="SEQ" />
		<constructor-arg index="2" value="INTEGER" />
	</bean>

	<bean id="lhs.column.code" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="2" />
		<constructor-arg index="1" value="CODE" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="lhs.column.name" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="3" />
		<constructor-arg index="1" value="NAME" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="lhs.column.spec" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="4" />
		<constructor-arg index="1" value="SPEC" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="lhs.column.count" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="5" />
		<constructor-arg index="1" value="COUNT" />
		<constructor-arg index="2" value="INTEGER" />
	</bean>

	<bean id="lhs.column.ware_house" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="6" />
		<constructor-arg index="1" value="WARE_HOUSE" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="lhs.column.delivery_date" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="7" />
		<constructor-arg index="1" value="DELIVERY_DATE" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<!-- YF database config -->
	<bean id="rhs.source" class="org.diffkit.diff.sns.DKDBSource">
		<constructor-arg index="0" value="coptd" />
		<constructor-arg index="1" value="WHERE CREATE_DATE > '20160801000000000'" />
		<constructor-arg index="2" ref="yfConnectionSource" />
		<constructor-arg index="3" ref="rhs.table.model" />
		<constructor-arg index="4">
			<list>
				<value>TD002</value>
				<value>TD004</value>
			</list>
		</constructor-arg>
		<constructor-arg index="5"><null /></constructor-arg>
	</bean>

	<bean id="rhs.table.model" class="org.diffkit.diff.engine.DKTableModel">
        <constructor-arg index="0" value="coptd" />
		<constructor-arg index="1">
			<list>
				<ref bean="rhs.column.order_code" />
				<ref bean="rhs.column.seq" />
				<ref bean="rhs.column.code" />
				<ref bean="rhs.column.name" />
				<ref bean="rhs.column.spec" />
				<ref bean="rhs.column.count" />
				<ref bean="rhs.column.ware_house" />
				<ref bean="rhs.column.delivery_date" />
			</list>
		</constructor-arg>
		<constructor-arg index="2">
			<list>
				<value>0</value>
			</list>
		</constructor-arg>
	</bean>

	<bean id="rhs.column.order_code" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="0" />
		<constructor-arg index="1" value="TD002" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="rhs.column.seq" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="1" />
		<constructor-arg index="1" value="TD003" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="rhs.column.code" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="2" />
		<constructor-arg index="1" value="TD004" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="rhs.column.name" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="3" />
		<constructor-arg index="1" value="TD005" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="rhs.column.spec" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="4" />
		<constructor-arg index="1" value="TD006" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="rhs.column.count" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="5" />
		<constructor-arg index="1" value="TD008" />
		<constructor-arg index="2" value="INTEGER" />
	</bean>

	<bean id="rhs.column.ware_house" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="6" />
		<constructor-arg index="1" value="TD007" />
		<constructor-arg index="2" value="STRING" />
	</bean>

	<bean id="rhs.column.delivery_date" class="org.diffkit.diff.engine.DKColumnModel">
		<constructor-arg index="0" value="7" />
		<constructor-arg index="1" value="TD013" />
		<constructor-arg index="2" value="STRING" />
	</bean>

</beans>
